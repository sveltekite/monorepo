{
	"name": "@sveltekite/legacy",
	"version": "0.0.1",
	"scripts": {
		"sail": "node bin/sveltekit-sails init",
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"bin": {
		"sveltekit-sails": "./bin/sveltekit-sails"
	},
	"files": [
		"bin/",
		"dist/",
		"templates/",
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"publint": "^0.3.12",
		"svelte": "^5.36.10",
		"svelte-check": "^4.3.0",
		"typescript": "^5.8.3",
		"vite": "^7.0.5"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^24.0.15",
		"chalk": "^5.4.1",
		"commander": "^14.0.0",
		"dexie": "^4.0.11",
		"inquirer": "^12.7.0",
		"js-yaml": "^4.1.0",
		"zod": "^4.0.5"
	}
}
